{
  "version": 2,
  "buildCommand": "bash build.sh",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "build/client",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/index" }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
